      <trans-unit id="closing-1" datatype="html">
        <source> V: Pray for us, O holy Mother of God. R: That we may be made worthy of the promises of Christ.
</source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/prayers/closing/closing.component.html</context>
          <context context-type="linenumber">2,5</context>
